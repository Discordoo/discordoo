{
  "description": "Discordoo is a new level node.js library for interacting with the Discord API.",
  "private": true,
  "scripts": {
    "build": "lerna run build",
    "bootstrap": "lerna bootstrap",
    "deploy": "lerna publish",
    "lint": "eslint packages/**/src/**/*",
    "lint:fix": "eslint packages/**/src/**/* --fix",
    "test": "jest",
    "docs": "typedoc ./packages/discordoo/src/index.ts --readme README.md --tsconfig ./tsconfig.json --exclude \"*.d.ts\" --json ./docs.json",
    "docs:html": "typedoc ./packages/discordoo/src/index.ts --readme README.md --tsconfig ./tsconfig.json --exclude \"*.d.ts\""
  },
  "keywords": [
    "api",
    "discord",
    "bot"
  ],
  "contributors": [
    {
      "name": "Mirdukkk"
    },
    {
      "name": "KislBall"
    }
  ],
  "workspaces": [
    "packages/*"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.25",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "eslint": "^7.20.0",
    "jest": "^26.6.3",
    "lerna": "^3.22.1",
    "pre-commit": "^1.2.2",
    "ts-jest": "^26.5.1",
    "tslib": "^2.1.0",
    "typedoc": "^0.20.25",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "lodash": "^4.17.20"
  },
  "precommit": [
    "lint",
    "test"
  ]
}
